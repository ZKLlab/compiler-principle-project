var a, b, c, r0, r1, r2, r3;
begin
    r0 := 1;
    r1 := 0;
    r2 := 0;
    r3 := 0;
    read(a, b, c);
    if a <= 0 then r0 := 0;
    if b <= 0 then r0 := 0;
    if c <= 0 then r0 := 0;
    if a <= b + c then r0 := 0;
    if b <= a + c then r0 := 0;
    if c <= a + b then r0 := 0;
    if r0 = 1 then
        begin
            if a = b then if b = c then r1 := 1;
            if a = b then r2 := 1;
            if a = c then r2 := 1;
            if b = c then r2 := 1;
            if a * a = b * b + c * c then r3 := 1;
            if b * b = a * a + c * c then r3 := 1;
            if c * c = a * a + b * b then r3 := 1;
        end;
    write(r0, r1, r2, r3);
end.